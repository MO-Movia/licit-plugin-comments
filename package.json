{
  "name": "@modusoperandi/licit-comment",
  "version": "0.0.9",
  "subversion": "1",
  "description": "Comment plugin built with React and ProseMirror",
  "main": "dist/index.js",
  "style": "dist/styles.css",
  "scripts": {
    "test": "jest --coverage",
    "build:clean": "rm -rf dist/ && rm -f modusoperandi-licit-comment-*.*.*.tgz",
    "webpack": "webpack",
    "build:babel": "babel src --out-dir dist",
    "build:dist": "npm run build:clean && npm run webpack && npm run build:babel && npm run build:css",
    "build:css": "cp src/*.css dist/",
    "flow": "flow --show-all-errors",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:js": "eslint \"src/**/*.js\"",
    "prepare": "npm run build:dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/plugin-proposal-do-expressions": "^7.16.0",
    "@babel/plugin-proposal-export-default-from": "^7.16.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.0",
    "@babel/plugin-proposal-function-sent": "^7.16.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.16.0",
    "@babel/plugin-proposal-throw-expressions": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-flow-strip-types": "^7.16.0",
    "@babel/plugin-transform-parameters": "^7.16.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-flow": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "8.2.3",
    "babel-plugin-flow-react-proptypes": "26.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "7.8.1",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.20.3",
    "file-loader": "^6.2.0",
    "flow-bin": "0.165.1",
    "flow-copy-source": "2.0.9",
    "flow-webpack-plugin": "1.2.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-prosemirror": "^1.0.10",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@modusoperandi/licit-ui-commands": "^0.1.5",
    "@pubsweet/ui-toolkit": "^2.3.17",
    "@remirror/core-utils": "^1.1.3",
    "@types/react-dom": "^17.0.11",
    "browserkeymap": "2.0.2",
    "classnames": "2.3.1",
    "color": "3.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "exports-loader": "3.1.0",
    "invariant": "2.2.4",
    "inversify": "^6.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "katex": "0.15.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "prosemirror-commands": "1.1.12",
    "prosemirror-dropcursor": "^1.3.5",
    "prosemirror-gapcursor": "1.1.5",
    "prosemirror-history": "1.1.3",
    "prosemirror-inputrules": "1.1.3",
    "prosemirror-keymap": "^1.1.4",
    "prosemirror-model": "^1.13.0",
    "prosemirror-state": "^1.3.4",
    "prosemirror-transform": "^1.2.9",
    "prosemirror-utils": "^1.0.0-0",
    "prosemirror-view": "^1.17.3",
    "query-string": "6.13.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-tooltip": "4.0.3",
    "react-transition-group": "^2.5.1",
    "resize-observer-polyfill": "1.5.1",
    "smooth-scroll-into-view-if-needed": "1.1.32",
    "stable": "0.1.8",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.3",
    "use-deep-compare-effect": "^1.8.1",
    "uuid": "8.3.2"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "!(*test|*.setup).js": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-prosemirror/environment"
    ],
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "*.js"
    ],
    "verbose": true,
    "testTimeout": 30000,
    "rootDir": "src",
    "testRegex": "((\\.|/*.)(test))\\.js?$",
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "../jest.setup.js"
    ]
  }
}
